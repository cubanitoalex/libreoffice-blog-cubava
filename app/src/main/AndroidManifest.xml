    <application
        android:allowBackup="true"
        android:icon="@drawable/libreo_cuba"
        android:label="@string/app_name"
        android:roundIcon="@drawable/libreo_cuba"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">|1
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WebViewResposive"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />
        <!--
            <activity android:name="cu.cubava.libreoffice.WebViewResposive"
            android:theme="@style/AppTheme.NoActionBar"/>
        -->

        <activity
            android:name=".Acercade"
            android:label="@string/title_activity_acercade"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>